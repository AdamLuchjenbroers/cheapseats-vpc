{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Sid": "BuildDataAccess",
            "Effect": "Allow",
            "Action": [
                "s3:DeleteObjectTagging",
                "s3:ListBucket",
                "s3:PutObject",
                "s3:GetObject",
                "s3:PutBucketAcl",
                "s3:PutBucketPolicy",
                "s3:GetObjectTagging",
                "s3:PutObjectTagging",
                "s3:DeleteObject",
                "s3:PutObjectAcl",
                "kms:Encrypt",
                "kms:DescribeKey"
            ],
            "Resource": [
                "arn:aws:s3:::codepipeline-ap-southeast-2-810622726934",
                "arn:aws:s3:::codepipeline-ap-southeast-2-810622726934/*",
                "arn:aws:kms:*:495602461790:key/*"
            ]
        },
        {
            "Sid": "BuildSSMAccess",
            "Effect": "Allow",
            "Action": [
                "ssm:GetParameters",
                "ssm:GetParameter"
            ],
            "Resource": [
                "arn:aws:ssm:*:495602461790:parameter/Deimos/*",
                "arn:aws:ssm:*:495602461790:parameter/AccountEnv/*"
            ]
        }
    ]
}
